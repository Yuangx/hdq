package repos

import (
	"reflect"
	"testing"

	"github.com/qiniu/hdq"
	_ "github.com/qiniu/hdq/stream/zip"
)

func _TestNew(t *testing.T) {
	doc := hdq.Source("zip://data.zip#index.htm")
	ret := New(doc)
	expected := Result{Next: "abc"}
	if !reflect.DeepEqual(ret, expected) {
		t.Fatal("Test failed:", ret, expected)
	}
}
